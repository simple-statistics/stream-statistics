#!/usr/bin/env node

var ss = (require('../'))();

process.stdin.setEncoding('ascii');

process.stdin.resume();

process.stdin.on('data', function(chunk){
    chunk
        .split(/\n|\s/g)
        .map(parseFloat)
        .filter(function(x) { return !isNaN(x); })
        .map(ss.write.bind(ss));
});

process.stdin.on('end', function(){
	var stats = ss.sample();
    'count sum mean min max standard_deviation variance skewness kurtosis'.split(' ').map(function(s) {
        console.log(s + ' = ' + stats[s]());
    });
});
